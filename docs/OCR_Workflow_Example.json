{
  "format": "pedantic-workflow-v1",
  "metadata": {
    "name": "OCR Text Extraction Workflow",
    "description": "Demonstrates OCR functionality - captures a screenshot from a website and extracts text using OCR",
    "tags": ["ocr", "browser", "text-extraction", "screenshot", "tesseract"],
    "exportedAt": "2025-01-27T00:00:00.000Z",
    "version": "1.0.0"
  },
  "workflow": {
    "nodes": {
      "start_1": {
        "type": "start",
        "title": "Start",
        "position": { "x": 100, "y": 200 }
      },
      "python_prepare_url": {
        "type": "python",
        "title": "Prepare URL",
        "code": "def run(input):\n    # URL with text content to extract\n    # Using a simple HTML page with visible text\n    return {\n        'url': 'https://httpbin.org/html',\n        'test_name': 'OCR Text Extraction'\n    }",
        "position": { "x": 300, "y": 200 }
      },
      "browser_capture": {
        "type": "browser",
        "title": "Capture Screenshot",
        "config": {
          "url": "{url}",
          "headless": true,
          "stealth_mode": false,
          "wait_for": "network_idle",
          "wait_selector": "",
          "wait_timeout": 30000,
          "output_formats": ["screenshot"],
          "json_extraction": {
            "method": "css",
            "selectors": {},
            "ai_prompt": ""
          },
          "session_id": "ocr_test",
          "viewport": {
            "width": 1920,
            "height": 1080
          },
          "user_agent": "default",
          "custom_user_agent": "",
          "timeout": 60000
        },
        "position": { "x": 500, "y": 200 }
      },
      "ocr_extract_text": {
        "type": "ocr",
        "title": "Extract Text with OCR",
        "config": {
          "content_key": "screenshot",
          "language": "eng",
          "psm": 6
        },
        "position": { "x": 700, "y": 200 }
      },
      "image_view_screenshot": {
        "type": "image",
        "title": "View Screenshot",
        "config": {
          "content_key": "screenshot"
        },
        "position": { "x": 500, "y": 400 }
      },
      "json_view_ocr_results": {
        "type": "json",
        "title": "View OCR Results",
        "config": {
          "content_key": "text, confidence, word_count"
        },
        "position": { "x": 900, "y": 200 }
      },
      "end_1": {
        "type": "end",
        "title": "End",
        "position": { "x": 1100, "y": 200 }
      }
    },
    "connections": {
      "conn_1": {
        "source": "start_1",
        "target": "python_prepare_url",
        "sourceOutput": "output",
        "targetInput": "input"
      },
      "conn_2": {
        "source": "python_prepare_url",
        "target": "browser_capture",
        "sourceOutput": "output",
        "targetInput": "input"
      },
      "conn_3": {
        "source": "browser_capture",
