{
  "name": "visual-agentic-workflow-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:api\"",
    "dev:next": "next dev",
    "dev:api": "cd api && python3 -m uvicorn simple_main:app --reload --host 0.0.0.0 --port 8000",
    "build": "next build",
    "prod": "next build && concurrently \"npm run start\" \"npm run dev:api\"",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "cd api && pip install -r requirements.txt",
    "run-workflow": "ts-node scripts/run-workflow.ts"
  },
